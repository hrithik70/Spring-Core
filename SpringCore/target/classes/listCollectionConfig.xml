<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans
         http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean
		class="com.hrithik.SpringCore.DependencyInjectionCollection.List.TataMotors"
		name="company">
		<property name="id" value="1" />
		<property name="companyName" value="Tata" />
		<property name="Cars">
			<list>
				<bean
					class="com.hrithik.SpringCore.DependencyInjectionCollection.List.Cars"
					name="Nano">
					<property name="carId" value="123" />
					<property name="model" value="Tata Nano" />
					<property name="price" value="300000" />
				</bean>

				<bean
					class="com.hrithik.SpringCore.DependencyInjectionCollection.List.Cars"
					name="Safari">
					<property name="carId" value="183" />
					<property name="model" value="Tata Safari" />
					<property name="price" value="900000" />
				</bean>

				<bean
					class="com.hrithik.SpringCore.DependencyInjectionCollection.List.Cars"
					name="Nexon">
					<property name="carId" value="173" />
					<property name="model" value="Tata Nexon" />
					<property name="price" value="500000" />
				</bean>
			</list>
		</property>
	</bean>






</beans>